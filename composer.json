{
    "name": "pingencom/pingen2-sdk-php",
    "type": "library",
    "license": "BSD-3-Clause",
    "description": "Official PHP Pingen SDK for API V2",
    "config": {
        "platform": {
            "php": "7.4.0"
        }
    },
    "require": {
        "php": "^7.4|^8.0|^8.1|^8.2",
        "ext-json": "*",
        "league/oauth2-client": "2.5.0",
        "illuminate/support": "7.12.0",
        "illuminate/http": "^7.12.0"
    },
    "autoload": {
        "psr-4": {
            "Pingen\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Pingen GmbH",
            "email": "info@pingen.com"
        }
    ],
    "require-dev": {
        "php-parallel-lint/php-parallel-lint": "^1.2",
        "nikic/php-parser": "4.15.*",
        "phpunit/phpunit": "^9.3",
        "mockery/mockery": "^1.4",
        "symplify/easy-coding-standard-prefixed": "9.2.6",
        "rector/rector": "^0.12.16",
        "phpstan/phpstan": "^1.4"
    }
}
